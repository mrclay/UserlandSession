{
    "name": "mrclay/userland-session",
    "type": "library",
    "description": "Provides a an HTTP cookie-based session in plain PHP, allowing concurrent use with existing native sessions.",
    "keywords": ["session", "HTTP", "SSO", "single sign-on"],
    "homepage": "https://github.com/mrclay/UserlandSession",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Clay",
            "homepage": "http://www.mrclay.org/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "paragonie/random_compat": "^2.0"
    },
    "require-dev": {
        "php": ">=5.3.0",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^3.7"
    },
    "autoload": {
        "psr-0": {"UserlandSession\\": "src/"}
    },
    "autoload-dev": {
        "psr-0": {"UserlandSession\\": "tests/"}
    }
}
